		"eslint-plugin-prettier": "^5.0.1",
		"eslint-plugin-vue": "^9.18.1",
		"prettier": "^3.0.2",
		"sass": "^1.66.1",
		"typescript": "^5.0.2",
		"vite": "^4.4.5",
		"vite-plugin-eslint": "^1.8.1",
