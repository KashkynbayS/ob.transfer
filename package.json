		"axios": "^1.6.2",
		"jwt-decode": "^4.0.0",
		"luxon": "^3.4.4",
		"pinia": "^2.1.6",
		"single-spa-vue": "^3.0.0",
		"vee-validate": "^4.11.8",
		"volar-service-vetur": "^0.0.30",
