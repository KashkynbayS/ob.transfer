{
	"name": "ob.open-account.ui",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc && vite build",
		"build:dev": "vue-tsc && vite build --mode dev",
		"build:staging": "vue-tsc && vite build --mode staging",
		"build:prod": "vue-tsc && vite build --mode master",
		"preview": "vite preview",
		"lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore --fix src",
		"format": "prettier --write src/",
		"update": "npm i @ui-kit/kmf-icons @ui-kit/kmf-styles @ui-kit/ui-kit"
	},
	"dependencies": {
		"@ui-kit/kmf-icons": "^0.1.3",
		"@ui-kit/kmf-styles": "^0.1.25",
		"@ui-kit/ui-kit": "^1.0.4",
		"@vee-validate/yup": "^4.11.8",
		"axios": "^1.5.0",
		"pinia": "^2.1.6",
		"single-spa-vue": "^3.0.0",
		"vee-validate": "^4.11.8",
		"vue": "^3.3.4",
		"vue-i18n": "9.5.0",
		"vue-router": "^4.2.4"
	},
	"devDependencies": {
		"@types/node": "^20.5.2",
		"@typescript-eslint/eslint-plugin": "^6.4.1",
		"@typescript-eslint/parser": "^6.4.1",
		"@vitejs/plugin-vue": "^4.2.3",
		"@vue/eslint-config-prettier": "^8.0.0",
		"@vue/eslint-config-typescript": "^11.0.3",
		"eslint": "^8.47.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-vue": "^9.15.0",
		"prettier": "^3.0.2",
		"sass": "^1.66.1",
		"typescript": "^5.0.2",
		"vite": "^4.4.5",
		"vite-plugin-eslint": "^1.8.1",
		"vite-svg-loader": "^4.0.0",
		"vue-eslint-parser": "^9.3.1",
		"vue-tsc": "^1.8.5"
	}
}